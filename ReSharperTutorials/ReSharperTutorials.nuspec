<?xml version="1.0" encoding="utf-8"?>
<package>
	<metadata>
		<id>JetBrains.ReSharperTutorials</id>
		<version>0.9.13</version>
		<authors>JetBrains</authors>
		<owners>JetBrains</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>
			A set of interactive tutorials for learning JetBrains ReSharper. To run, use the menu ReSharper | Tutorials...
		</description>		
		<dependencies>
			<dependency id="Wave" version="[8.0]" />
		</dependencies>
	</metadata>
	<files>
		<file src="Tutorials\**\*.*" target="Tutorials\" />
		<file src="TutorialsContent\**\*.*" target="Tutorials\Content\" />
		<file src="Tutorials\**\*.*" target="WorkingCopy\" />
		<file src="TutorialsContent\**\*.*" target="WorkingCopy\Content\" />
		<file src="bin\Debug\ReSharperTutorials.dll" target="dotFiles\" />
		<file src="bin\Debug\ReSharperTutorials.pdb" target="dotFiles\" />
	</files>
</package>